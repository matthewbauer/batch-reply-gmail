{
  "name": "Batch Reply for GMail",
  "description": "Adds button to reply to all selected conversations on Gmailâ„¢.",
  "version": "7",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["inboxsdk.js", "reply.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "reply.png"
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "manifest_version": 2
}
